{
  "name": "canopi-backend",
  "version": "1.0.0",
  "description": "Canopi - Trading Bot Backend API",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "nodemon --exec ts-node src/index.ts",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:migrate:json": "tsx src/migrations/migrate-from-json.ts"
  },
  "keywords": [
    "solana",
    "trading",
    "bot",
    "defi"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@electric-sql/pglite": "^0.3.14",
    "@radr/shadowwire": "^1.1.1",
    "@solana/spl-token": "^0.4.14",
    "@solana/web3.js": "^1.87.6",
    "@supabase/supabase-js": "^2.75.1",
    "axios": "^1.6.2",
    "bs58": "^5.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.44.6",
    "express": "^4.18.2",
    "node-cache": "^5.1.2",
    "node-telegram-bot-api": "^0.67.0",
    "pg": "^8.16.3",
    "socks-proxy-agent": "^8.0.5",
    "tweetnacl": "^1.0.3",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.19.21",
    "@types/node-telegram-bot-api": "^0.64.13",
    "@types/pg": "^8.15.5",
    "@types/ws": "^8.5.10",
    "drizzle-kit": "^0.31.5",
    "jest": "^30.2.0",
    "nodemon": "^3.0.2",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.3.3"
  }
}
